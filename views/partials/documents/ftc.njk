    Parent 1 - EUSS (Yes)
    Parent 2 - UK Passport (Yes)
    Parent 2 - EUSS (No)
-->
{% elif values['born-after-2006'] and values['born-in-uk'] and (values['parent1-uk-passport'] == 'No' and values['parent1-euss'] == true and values['parent2-uk-passport'] == 'Yes' and values['parent2-euss'] == false) %}
