    <hr>

<!-- OLC PDF 11 copy of 10 -->
{% elif values['born-after-2006'] and not values['born-in-uk'] and (not values['parent1-born-before-1983'] or not values['parent2-born-before-1983']) %}
    <h2 class="govuk-heading-l">Parents' documents</h2>
