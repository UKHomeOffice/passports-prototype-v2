<!-- OLC PDF 13 copy of 12 -->
{% elif values['born-after-2006'] and not values['born-in-uk'] and (values['parent1-born-before-1983'] and values['parent2-born-before-1983']) %}
    <h2 class="govuk-heading-l">Parents' documents</h2>
